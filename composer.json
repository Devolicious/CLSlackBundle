{
  "name": "cleentfaar/slack-bundle",
  "description": "This bundle provides integration with the Slack library, allowing you to interact with the Slack API within your Symfony projects",
  "keywords": ["slack", "api", "symfony", "bundle"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": "~2.4",
    "symfony/console": "~2.4",
    "cleentfaar/slack": "~0.9.4",
    "cleentfaar/humanizer": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1"
  },
  "autoload": {
    "psr-0": {
      "CL\\Bundle\\SlackBundle": ""
    }
  },
  "target-dir": "CL/Bundle/SlackBundle"
}
